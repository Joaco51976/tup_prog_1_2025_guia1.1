Proceso sin_titulo
	definir quimicoA, quimicoB Como Real;
	definir litrosQA, litrosQB Como Entero;
	definir totalqA, totalqB Como Real;
	
	escribir "cuantos gramos de quimico A usa por litro?";
	leer quimicoA;
	escribir "cuantos gramos de quimico B usa por litro?";
	leer quimicoB;
	
	escribir "cuantos litros del compuesto 1 (A) tiene disponible?";
	leer litrosQA;
	escribir "cuantos litros del compuesto 2 (B) tiene disponible?";
	leer litrosQB;
	
	totalqA <- quimicoA * litrosQA;
	totalqB <- quimicoB * litrosQB;
	
	Escribir "usted necesita:", totalqA , " gramos de quimico A para los litros de compuesto 1";
	Escribir "usted necesita:", totalqB , " gramos de quimico B para los litros de compuesto 2";
	
FinProceso